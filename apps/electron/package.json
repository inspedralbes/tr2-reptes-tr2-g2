{
    "name": "electron",
    "version": "1.0.0",
    "main": "main.mjs",
    "type": "module",
    "scripts": {
        "dev": "wait-on http://localhost:3001 && cross-env VITE_DEV_SERVER_URL=http://localhost:3001 electron .",
        "build": "electron-builder"
    },
    "build": {
        "appId": "com.iter.desktop",
        "productName": "Iter Ecosystem",
        "directories": {
            "output": "dist"
        },
        "files": [
            "main.mjs",
            "preload.mjs",
            "assets/**/*",
            "../web/out/**/*"
        ],
        "win": {
            "target": "nsis"
        }
    },
    "dependencies": {},
    "devDependencies": {
        "electron": "^33.0.0",
        "electron-builder": "^25.1.8"
    }
}